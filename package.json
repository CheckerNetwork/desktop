{
  "name": "filecoin-station",
  "productName": "Filecoin Station",
  "private": true,
  "version": "0.0.5",
  "description": "Desktop application for Filecoin Retrieval Markets",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "lint": "standard",
    "test": "shx echo \"WARN: no test specified\"",
    "test:e2e": "shx echo \"WARN: no e2e tests specified\"",
    "postinstall": "run-s install-app-deps",
    "install-app-deps": "electron-builder install-app-deps",
    "clean": "rimraf dist renderer/.next renderer/out",
    "build": "next build renderer && next export renderer",
    "package": "shx rm -rf dist/ && run-s build && electron-builder --publish onTag"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filecoin-project/filecoin-station.git"
  },
  "keywords": [
    "filecoin"
  ],
  "author": "Protocol Labs",
  "license": "(Apache-2.0 AND MIT)",
  "bugs": {
    "url": "https://github.com/filecoin-project/filecoin-station/issues"
  },
  "homepage": "https://github.com/filecoin-project/filecoin-station#readme",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.6",
    "electron-next": "^3.1.5",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5"
  },
  "devDependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.5",
    "autoprefixer": "^10.4.2",
    "babel-plugin-postcss": "^1.1.0",
    "cross-env": "^7.0.3",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-notarize": "^1.1.1",
    "next": "^12.1.0",
    "next-themes": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gauge-chart": "^0.4.0",
    "shx": "^0.3.4",
    "standard": "^16.0.4",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1"
  }
}
